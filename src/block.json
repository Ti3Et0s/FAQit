{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/faqit",
	"version": "0.1.0",
	"title": "faqit",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "faqit",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"attributes": {
		"faqs": {
			"type": "array",
			"default": []
		},
		"content": {
			"type": "rich-text",
			"source": "html",
			"selector": "h2"
		},
		"arrowColor": {
			"type": "string",
			"default": "#000000"
		}
	}
}
